<mvc:View
	controllerName="smbproject1.0.controller.Home"
	xmlns="sap.m"
	xmlns:m="sap.suite.ui.microchart"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:layout="sap.ui.layout"
	xmlns:f="sap.f"
	xmlns:card="sap.f.cards"
>
	<Page id="home" title="Standorte">
		<headerContent>
			<Image
				src="img/Logo-SMB.png"
				height="80%"
			/>
			<ToolbarSpacer/>
			<Toolbar>
				<Label
					labelFor="DRS1"
					text="Datum auswählen:"
					width="65%"
				/>
				<DateRangeSelection
					id="DRS1"
					delimiter="{
						path: '/delimiterDRS1'
					}"
					dateValue="{
						path: '/dateValueDRS1'
					}"
					secondDateValue="{
						path: '/secondDateValueDRS1'
					}"
					displayFormat="{
						path: '/dateFormatDRS1'
					}"
					
				/>
			</Toolbar>
		</headerContent>
		<!-- <Title
			level="H1"
			titleStyle="H1"
			id="_IDGenTitle1"
			text="Standorte"
			class="sapUiTinyMarginBegin sapUiMediumMarginTop"
		/> -->
		<layout:VerticalLayout
			width="100%"
			class="gridWrapper"
		>
			<layout:Grid containerQuery="true">
				<f:Card headerPosition="Top">
					<f:header>
						<card:Header
							title="Standort Chur"
							subtitle="Nr. 2050"
							iconSrc="./img/SMB-Chur.png"
                            press=".onChartPressed"
						/>
					</f:header>
					<f:content>
						<VBox
							height="10rem"
							class="sapUiSmallMargin"
							justifyContent="SpaceBetween"
						>
							<HBox justifyContent="SpaceAround">
								<FlexBox
									width="20rem"
									height="10rem"
									alignItems="Start"
									justifyContent="SpaceBetween"
								>
									<items>
										<m:InteractiveDonutChart>
											<m:segments>
												<m:InteractiveDonutChartSegment
													label="Ausgeführt"
													value="40.0"
													displayedValue="40.0%"
												/>
												<m:InteractiveDonutChartSegment
													label="in Bearbeitung"
													value="21.5"
													displayedValue="21.5%"
												/>
												<m:InteractiveDonutChartSegment
													label="erfasst"
