<mvc:View controllerName="smbproject1.0.controller.SecondPage" xmlns="sap.m" xmlns:m="sap.suite.ui.microchart" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout" xmlns:f="sap.f" xmlns:card="sap.f.cards" xmlns:core="sap.ui.core" xmlns:table="sap.ui.table" xmlns:sfi="sap.ui.comp.smartfield">
    <Page id="secondPage">
        <headerContent>
            <Image src="img/Logo-SMB.png" height="80%" />
            <ToolbarSpacer />
            <Toolbar>
                <Label labelFor="DRS1" text="Datum" />
                <DateRangeSelection
                    id="DRS1"
                    delimiter="{
                    path: '/delimiterDRS1'
                }"
                    dateValue="{
                    path: '/dateValueDRS1'
                }"
                    secondDateValue="{
                    path: '/secondDateValueDRS1'
                }"
                    displayFormat="{
                    path: '/dateFormatDRS1'
                }"
                />
            </Toolbar>
        </headerContent>
        <layout:VerticalLayout id="_IDGenVerticalLayout1" class="sapUiResponsiveMargin">
            <Title id="_IDGenTitle1" titleStyle="H2" text="Chur" class="sapUiTinyMarginBegin sapUiMediumMarginTop" />
            <layout:HorizontalLayout id="_IDGenHorizontalLayout1" allowWrapping="false">
                
            </layout:HorizontalLayout>
        </layout:VerticalLayout>
    </Page>
</mvc:View>
